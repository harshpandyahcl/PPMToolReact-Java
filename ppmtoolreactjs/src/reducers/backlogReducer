import {GET_BACKLOG, GET_PROJECT_TASK, DELETE_PROJECT_TASK} from "../actions/types";


const initialState = {
    projet_tasks:[],
    projet_tasks:{}
}

export default function(state = initialState, action){
    switch(action.type){
        
        case GET_BACKLOG:
            return{
               ...state,
                projet_tasks:action.payload
        }
        case GET_PROJECT_TASK:
            return{
                ...state,
                projet_tasks:action.payload
            }
        case DELETE_PROJECT_TASK:
            return{
                ...state
            }
        
        default:
            return state;
    }
}